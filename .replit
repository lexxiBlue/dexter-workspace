modules = ["nodejs-20"]
[agent]
expertMode = true
integrations = ["github:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Push to GitHub"

[[workflows.workflow]]
name = "Push to GitHub"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run push"

[workflows.workflow.metadata]
outputType = "console"
